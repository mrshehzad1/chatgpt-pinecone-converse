
{
  "rewrites": [
    { "source": "/api/pinecone-proxy", "destination": "/api/pinecone-proxy.js" },
    { "source": "/api/pinecone-describe", "destination": "/api/pinecone-describe.js" },
    { "source": "/api/openai-proxy", "destination": "/api/openai-proxy.js" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "X-Requested-With, Content-Type, Accept" }
      ]
    }
  ]
}
